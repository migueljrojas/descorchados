footer.footer
    .container
        .row
            .col-md-8
                p.copyright= site.data.footer.footer.copyright
            .col-md-4
                ul
                    each item in site.data.footer.footer.items1
                        li
                            a(href=item.url)= item.name
            .col-md-4
                ul
                    each item in site.data.footer.footer.items2
                        li
                            a(href=item.url)= item.name
            .col-md-8.redes
                ul
                    each item in site.data.footer.footer.redes
                        li.network
                            a(href=item.url)
                                i(class=item.name)



